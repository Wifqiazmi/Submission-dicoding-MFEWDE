{
    "env": {
        "browser": true,
        "codeceptjs/codeceptjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
        
    ],
    "plugins": [
        "codeceptjs"
      ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0
    }
}
